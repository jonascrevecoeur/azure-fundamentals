{
  "scriptFile": "get_secret.py",
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route": "secret/{secret}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}